      function uc_icon_carousel_elementor26408_start(){

        var objCarousel = jQuery('#uc_icon_carousel_elementor26408');

        		objCarousel.owlCarousel({
      			loop: true,
                  center:false,
                  shuffle:false,
                  setActiveClassOnMobile:false,
                  changeItemOnClick:false,
                  rtl:false,
                  margin:24,
                  autoplay:true,
